
      <% include ../header %>

      <div class="panel panel-primary">
        <div class="panel-heading">Nova Imagem</div>
        <div class="panel-body">
        
      	<form role="form" method="POST" action="/admin/create" enctype="multipart/form-data">
          <div class="form-group">
            <label for="title">Título</label>
            <input type="text" class="form-control" id="title" name="title" placeholder="Título">
          </div>
          <div class="form-group">
            <label for="description">Descrição</label>
            <textarea name="description" id="description" class="form-control" rows="3" placeholder="Descrição"></textarea>
          </div>
          <div class="form-group">
            <label for="image">Imagem</label>
            <input type="file" id="image" name="image">
          </div>
          <button type="submit" class="btn btn-primary">Salvar</button>
          <a href="/admin" type="button" class="btn btn-default">Voltar</a>

        </form>

      </div>
    </div>


   
<% include ../footer %>
